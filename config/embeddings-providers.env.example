# Separate Embeddings Provider Configuration

# Embeddings Provider Configuration (Separate from Chat Provider)
# This allows using different providers for chat vs embeddings

# Embeddings Provider 1: Custom Embeddings Service
EMBEDDINGS_PROVIDER_1_NAME="Custom Embeddings"
EMBEDDINGS_PROVIDER_1_URL="https://your-embeddings-endpoint.com/v1"
EMBEDDINGS_PROVIDER_1_API_KEY="your-embeddings-api-key-here"
EMBEDDINGS_PROVIDER_1_MODEL="your-embedding-model-name"
EMBEDDINGS_PROVIDER_1_DIMENSION="1024"
EMBEDDINGS_PROVIDER_1_TIMEOUT="30000"
EMBEDDINGS_PROVIDER_1_MAX_RETRIES="3"

